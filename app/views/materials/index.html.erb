<p id="notice"><%= notice %></p>

<h1>Listing Materials</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @materials.each do |material| %>
      <tr>
        <td><%= link_to material.name, material %></td>
        <td><%= link_to 'See projects', material %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
